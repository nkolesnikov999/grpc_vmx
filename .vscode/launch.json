    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch PathInterfaces",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/PathInterfaces",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Launch main.go (root)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Attach to remote Delve (2345)",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 2345,
            "host": "127.0.0.1"
        }
    ]
}